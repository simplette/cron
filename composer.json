{
	"name": "simplette/cron",
	"version": "v0.1.0",
	"type": "library",
	"description": "Simple cron management for Nette Framework.",
	"keywords": [
		"nette",
		"console",
		"cron",
		"scheduled",
		"tasks"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Lukas Povoda",
			"email": "lupo112@gmail.com"
		}
	],
	"require": {
		"php": ">=5.6",
		"simplette/console": "~0.1",
		"nette/bootstrap": "~2.4",
		"nette/di": "~2.4",
		"nette/reflection": "~2.4",
		"nette/safe-stream": "~2.3",
		"nette/utils": "~2.4",
		"mtdowling/cron-expression": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.0",
		"mockery/mockery": "^0.9.8"
	},
	"autoload": {
